includes:
    - /vendor/nunomaduro/larastan/extension.neon

paths:
    - app
    - database
    - routes
    - tests
    - config
    - bootstrap

parameters:
    excludePaths:
        - bootstrap/cache
        - node_modules/
        - public/css
        - public/js
        - storage/
        - vendor/
        - tests/
    level: max
    editorUrl: 'phpstorm://open?file=%%file%%&line=%%line%%'
