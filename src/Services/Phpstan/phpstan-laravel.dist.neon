includes:
    - /vendor/nunomaduro/larastan/extension.neon
parameters:
    paths:
        - app
        - database
        - routes
        - tests
        - config
        - bootstrap
    excludePaths:
        - bootstrap/cache
        - node_modules/
        - public/css
        - public/js
        - storage/
        - vendor/
        - tests/
    level: max
    editorUrl: 'phpstorm://open?file=%%file%%&line=%%line%%'
    reportUnmatchedIgnoredErrors: true

